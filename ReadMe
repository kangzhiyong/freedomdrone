/Applications/CMake.app/Contents/bin/cmake .. -G "Xcode" -DPYTHON_INCLUDE_DIR=$(python -c "from distutils.sysconfig import get_python_inc; print(get_python_inc())")  -DPYTHON_LIBRARY=$(python -c "import distutils.sysconfig as sysconfig; print(sysconfig.get_config_var('LIBDIR'))") -DPYTHON_NUMPY_INCLUDE_DIR=$(python -c "from numpy import get_include; print(get_include())")